-
-// ========================
-//  SCROLL (lazy)
-// ========================
-function PT_bindScroll() {
-    const wrap = document.getElementById("product-table-wrapper");
-
-    wrap.addEventListener("scroll", () => {
-        if (wrap.scrollTop + wrap.clientHeight >= wrap.scrollHeight - 200) {
-            PT_load();
-        }
-    });
-}
